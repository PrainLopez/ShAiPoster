{
  "name": "shaiposter",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "pnpx convex dev --once && concurrently -r pnpm:dev:web pnpm:dev:convex",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "pnpx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@convex-dev/react-query": "0.0.0-alpha.11",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-router": "^1.134.12",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.134.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.2",
    "lucide-react": "^0.553.0",
    "openai": "^6.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.0.0",
    "@netlify/vite-plugin-tanstack-start": "^1.1.12",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/eslint-config": "^0.3.2",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "~9.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver"
    ]
  }
}